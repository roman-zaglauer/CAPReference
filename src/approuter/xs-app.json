{
  "welcomeFile": "app-a/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/-/cds/.*",
      "destination": "mtx-api",
      "authenticationType": "none"
    },
    {
      "source": "^/service/(.*)$",
      "target": "/service/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "csrfProtection": true
    }
  ]
}
